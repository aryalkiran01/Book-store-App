{
    "version": 2,
    "builds": [
      {
        "src": "main.ts",
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/dist/$1"
      }
    ]
  }
  